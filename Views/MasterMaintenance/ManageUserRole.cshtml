<div id="userrole">
    <div class="col-md-12">
         <div class="card ">
            <div class="card-header">
                <div class="float-left">
                    <div class="input-group input-group-sm ">                        
                        <input type="text" class="form-control" placeholder="Username or Role"data-bind="value: keyword">
                    </div>
                </div>
                <div class="btn-group float-right shadow">
                    <a href="#" class="btn btn-default btn-sm " data-bind="click: Search"><i
                            class="fa-solid fa-magnifying-glass text-info"></i> Search</a>
                    <a href="#" class="btn btn-default btn-sm " data-bind="click: Add"><i class="fas fa-light fa-pencil text-warning"></i>Add</a>

                </div>
            </div>
            <div class="card-body table-responsive p-0">
                <table class="table table-condensed table-striped table-hover text-nowrap">
                    <thead>
                        <tr>
                            <th>EmployeeID</th>
                            <th>Role Code</th>
                            <th>Status</th>
                            <th>Created By</th>
                            <th>Created Date</th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody data-bind="foreach: userRoles">                        
                        <tr>
                            <td data-bind="text: employeeId"></td>
                            <td data-bind="text: roleCode"></td>
                            <td data-bind="text: status"></td>
                            <td data-bind="text: createdBy"></td>
                            <td data-bind="text: createdTime"></td>
                            <td></td>
                        </tr>                           
                    </tbody>
                </table>
            </div>
         </div>
    </div>
</div>


<script type="text/javascript" src="~/js/onlinerfa_master_user_role.js"></script>